{
  "name": "@questcoder/backend",
  "version": "1.0.0",
  "description": "QuestCoder backend API server",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx --tsconfig-paths watch src/index.ts",
    "build": "tsc && tsc-alias && node --enable-source-maps dist/scripts/convertCsv.js",
    "start": "node dist/index.js",
    "convert-csv": "tsx --tsconfig-paths src/scripts/convertCsv.ts",
    "seed-badges": "tsx --tsconfig-paths scripts/seedBadges.ts",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.0",
    "node-cron": "^3.0.3",
    "socket.io": "^4.7.5",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "@types/node-cron": "^3.0.11",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "mongodb",
    "api"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
